{
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/assets/css"
    },

    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/assets/css"
    }
  ],
  "liveSassCompile.settings.excludeList": [
    "/**/node_modules/**",
    "/.vscode/**",
    "/assets/vendors/"
  ],
  "python.analysis.typeCheckingMode": "off"
}
